{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://study-badge.shop/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://study-badge.shop/:path*/"
    }
  ],
  "redirects": [
    {
      "source": "/api/(.*)",
      "destination": "https://study-badge.shop/api/$1",
      "permanent": true
    }
  ]
}
